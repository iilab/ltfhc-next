{
  "fields": {
    "date_incident": {
      "type": "date",
      "dateFormat": "yy-mm-dd",
      "datepicker": {
        "hideIfNoPrevNextType": true,
        "yearRange": "1900:+1",
        "changeMonth": true,
        "changeYear": true
      },
      "label": "When did the incident take place?"
    },
    "current_incident_days": {
      "type": "hidden"
    },
    "received_pep_kit": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Received PEP kit"
    },
    "received_pep_kit_72h": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Received PEP kit within 72h"
    },
    "received_emergency_contraception": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Received emergency contraception"
    },
    "hiv_tested": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Tested for HIV?"
    },
    "hiv_partner_test": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Partner tested for HIV?"
    },
    "hiv_positive_test": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Tested positive for HIV?"
    },
    "hiv_partner_positive_test": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Partner tested positive for HIV?"
    },
    "hiv_counseling": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "HIV counseling given?"
    },
    "hiv_partner_counseling": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "HIV counseling given to partner?"
    },
    "hiv_discordant": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "HIV discordant couple?"
    },
    "post_test_counseling": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Post-test HIV counseling given?"
    },
    "urogenital_fistula_post_rape": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Urogenital fistula post rape?"
    },
    "referred_to_elsewhere": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Referred to another facility?"
    },
    "referred_to_location": {
      "type": "select",
      "label": "If Yes, which facility was the patient referred to?"
    },
    "follow_up_recommended": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "A follow up visit has been recommended"
    }
  }
}