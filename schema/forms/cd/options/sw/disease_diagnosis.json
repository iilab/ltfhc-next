{
  "fields": {
    "complaint": {
      "type": "checkbox",
      "removeDefaultNone": true,
      "label": "Lalamiko kuu"
    },
    "complaint_other": {
      "label": "Kuu nyingine",
      "helper": "If Chief Complaint not listed above, please write in."
    },
    "symptom": {
      "type": "checkbox",
      "removeDefaultNone": true,
      "label": "Dalili"
    },
    "symptom_other": {
      "type": "textarea",
      "label": "Dalili nyingine"
    },
    "sexual_violence": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Visit related to Sexual violence"
    },
    "history": {
      "type": "checkbox",
      "removeDefaultNone": true,
      "label": "Historia ya zamani"
    },
    "history_other": {
      "type": "textarea",
      "label": "Historia ya zamani nyingine"
    },
    "note": {
      "type": "textarea",
      "label": "Examination Notes:"
    },
    "testing_method": {
      "type": "checkbox",
      "removeDefaultNone": true,
      "label": "Amepimwa kwa njia zifuatazo",
      "helper": "Please check the boxes in front of the tests that were used."
    },
    "testing_method_other": {
      "type": "textarea",
      "label": "Other method"
    },
    "testing_results": {
      "type": "checkbox",
      "removeDefaultNone": true,
      "label": "Positive Test results",
      "helper": "Please check the boxes in front of the tests which had positive results"
    },
    "testing_results_other": {
      "type": "textarea",
      "label": "Other test results"
    },
    "diagnosis": {
      "type": "select",
      "label": "Utambuzi wa ugonjwa"
    },
    "diagnosis_other": {
      "label": "Utambuzi wa ugonjwa nyingine"
    },
    "treatment": {
      "type": "textarea",
      "label": "Matibabu"
    },
    "admission": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Admission Required?"
    },
    "disease_mortality": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Je ugonjwa ulisababisha kifo?"
    },
    "referred_to_elsewhere": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Referred to another facility?"
    },
    "referred_to_location": {
      "type": "select",
      "label": "If Yes, which facility was the patient referred to?"
    },
    "follow_up_recommended": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "A follow up visit has been recommended"
    }
