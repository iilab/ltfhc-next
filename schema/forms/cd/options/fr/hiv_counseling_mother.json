{
  "fields": {
    "delivery_date": {
      "type": "date",
      "dateFormat": "yy-mm-dd",
      "datepicker": {
        "hideIfNoPrevNextType": true,
        "yearRange": "1900:+1",
        "changeMonth": true,
        "changeYear": true
      },
      "label": "Date of Infant Delivery:"
    },
    "first_visit": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "First Visit to Clinic?"
    },
    "hiv_known_pre_clinic": {
      "type": "radio",
      "enum": [
        "Yes",
        "No",
        "Unknown"
      ],
      "removeDefaultNone": true,
      "label": "HIV Status Known Before Clinic?"
    },
    "hiv_tested_pre_clinic": {
      "type": "radio",
      "enum": [
        "Yes",
        "No",
        "Unknown"
      ],
      "removeDefaultNone": true,
      "label": "Tested for HIV Before Clinic?"
    },
    "hiv_test": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "HIV Test given?"
    },
    "hiv_status": {
      "type": "radio",
      "removeDefaultNone": true,
      "label": "HIV Status"
    },
    "hiv_positive_first_test": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Tested HIV Positive on first test?"
    },
    "hiv_known_pre_birth": {
      "type": "radio",
      "enum": [
        "Yes",
        "No",
        "Unknown"
      ],
      "removeDefaultNone": true,
      "label": "HIV Status Known Before Birth?"
    },
    "hiv_test_during_labor": {
      "type": "radio",
      "enum": [
        "Yes",
        "No",
        "Unknown"
      ],
      "removeDefaultNone": true,
      "label": "Tested for HIV During Labor?"
    },
    "hiv_test_post_labor": {
      "type": "radio",
      "enum": [
        "Yes",
        "No",
        "Unknown"
      ],
      "removeDefaultNone": true,
      "label": "Tested for HIV Post-Labor?"
    },
    "medication_during_labor": {
      "label": "Medicine Taken During Labor:"
    },
    "referred_ctc": {
      "type": "radio",
      "enum": [
        "Yes",
        "No",
        "Unknown"
      ],
      "removeDefaultNone": true,
      "label": "Mother Referred To CTC?"
    },
    "referral_date": {
      "type": "date",
      "dateFormat": "yy-mm-dd",
      "datepicker": {
        "hideIfNoPrevNextType": true,
        "yearRange": "1900:+1",
        "changeMonth": true,
        "changeYear": true
      },
      "label": "Referral Date (Or is This a Different Date?):"
    },
    "arv_therapy": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Currently Taking Anti-Retrovial Therapy?"
    },
    "other_treatment": {
      "type": "textarea",
      "label": "If Any Treatment Given, Please Describe:"
    },
    "note": {
      "type": "textarea",
      "label": "Other Notes for This Visit:"
    },
    "referred_to_elsewhere": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Referred to another facility?"
    },
    "referred_to_location": {
      "type": "select",
      "label": "If Yes, which facility was the patient referred to?"
    },
    "follow_up_recommended": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Visite de suivi"
    }
  }
}