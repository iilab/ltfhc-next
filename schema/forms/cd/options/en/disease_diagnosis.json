{
  "fields": {
    "complaint": {
      "type": "checkbox",
      "removeDefaultNone": true,
      "label": "Chief Complaint:"
    },
    "complaint_other": {
      "label": "Other Complaint:",
      "helper": "If Chief Complaint not listed above, please write in."
    },
    "symptom": {
      "type": "checkbox",
      "removeDefaultNone": true,
      "label": "Symptoms:"
    },
    "symptom_other": {
      "type": "textarea",
      "label": "Other Symptoms:"
    },
    "sexual_violence": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Visit related to Sexual violence"
    },
    "history": {
      "type": "checkbox",
      "removeDefaultNone": true,
      "label": "History:"
    },
    "history_other": {
      "type": "textarea",
      "label": "Other Past History:"
    },
    "note": {
      "type": "textarea",
      "label": "Examination Notes:"
    },
    "testing_method": {
      "type": "checkbox",
      "removeDefaultNone": true,
      "label": "Tested By Following Method:",
      "helper": "Please check the boxes in front of the tests that were used."
    },
    "testing_method_other": {
      "type": "textarea",
      "label": "Other method"
    },
    "testing_results": {
      "type": "checkbox",
      "removeDefaultNone": true,
      "label": "Positive Test results",
      "helper": "Please check the boxes in front of the tests which had positive results"
    },
    "testing_results_other": {
      "type": "textarea",
      "label": "Other test results"
    },
    "diagnosis": {
      "type": "select",
      "label": "Disease Diagnosis:"
    },
    "diagnosis_other": {
      "label": "Other Disease Diagnosis:"
    },
    "treatment": {
      "type": "textarea",
      "label": "Treatment:"
    },
    "admission": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Admission Required?"
    },
    "disease_mortality": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Did Disease Lead To Death?"
    },
    "referred_to_elsewhere": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "Referred to another facility?"
    },
    "referred_to_location": {
      "type": "select",
      "label": "If Yes, which facility was the patient referred to?"
    },
    "follow_up_recommended": {
      "type": "radio",
      "enum": [
        "Yes",
        "No"
      ],
      "removeDefaultNone": true,
      "label": "A follow up visit has been recommended"
    }
  }
}