{
  "$schema": "http://json_schema.org/draft-04/schema#",
  "_id": "delivery_record",
  "title": "Delivery Record",
  "description": "",
  "type": "object",
  "properties": {
    "delivery_location": {
      "type": "string",
      "$ref": "./lists/#delivery_type"
    },
    "delivery_mode": {
      "type": "string",
      "$ref": "./lists/#delivery_mode"
    },
    "prenatal_problems": {
      "type": "string",
      "$ref": "./lists/#prenatal_prob"
    },
    "childbirth_complications": {
      "type": "string",
      "$ref": "./lists/#birth_comp"
    },
    "referred_to_elsewhere": {
      "type": "number",
      "enum": [
        "yes",
        "no"
      ]
    },
    "referred_to_location": {
      "type": "string",
      "$ref": "./lists/#referral_locations"
    },
    "follow_up_recommended": {
      "type": "number",
      "enum": [
        "yes",
        "no"
      ]
    }
  }
}